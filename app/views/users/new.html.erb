<h1>Signup</h1>

<font color= "rgb(88,88,88)">

<%= form_for User.new, url: '/signup' do |f| %>
<%= f.label :email %>
<%= f.email_field :email %>
<br>
<%= f.label :password %>
<%= f.password_field :password %>
<br>
<%= f.label :password_confirmation %>
<%= f.password_field :password_confirmation %>
<br>
<br>
<%= f.submit "Signup" %>
<% end %>
</font>